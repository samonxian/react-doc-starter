{
  "version": "0.0.3",
  "name": "business-utils",
  "author": "samonxian",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "dependencies": {
    "camelcase": "7.0.0"
  },
  "devDependencies": {
    "eslint": "8.18.0",
    "moment": "2.29.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.12"
  },
  "scripts": {
    "eslint": "eslint src/**/*.js src/**/*.ts src/**/*.jsx src/**/*.tsx --no-error-on-unmatched-pattern",
    "tsc:es": "tsc --declarationDir es",
    "tsc:lib": "tsc --declarationDir lib",
    "tsc": "npm run tsc:lib && npm run tsc:es",
    "clean": "rimraf lib es dist",
    "vite": "vite",
    "build:lib": "vite build",
    "build:file": "node ./scripts/buildFiles.js",
    "build": "npm run eslint && npm run clean && npm run tsc && npm run build:file && npm run build:lib"
  }
}
